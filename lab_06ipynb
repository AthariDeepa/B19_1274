      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "# Step 1: Create a sample dataset\n",
        "data = {\n",
        "    'PatientID': [101, 102, 103, 104, 105, 106, 107, 108],\n",
        "    'Age': [25, 34, -5, 150, 45, 77, 29, 130]  # Note: -5, 150, 130 are invalid\n",
        "}\n",
        "df = pd.DataFrame(data)\n",
        "print(\"Original Dataset:\\n\")\n",
        "print(df)\n",
        "# Step 2: Define outlier condition\n",
        "def is_valid_age(age):\n",
        "    return 0 <= age <= 120\n",
        "# Step 3: Detect outliers\n",
        "invalid_ages = df[~df['Age'].apply(is_valid_age)]\n",
        "print(\"\\nDetected Outliers (Invalid Ages):\\n\")\n",
        "print(invalid_ages)\n",
        "# Step 4: Remove outliers\n",
        "cleaned_df = df[df['Age'].apply(is_valid_age)]\n",
        "print(\"\\nCleaned Dataset (Outliers Removed):\\n\")\n",
        "print(cleaned_df)"
      ],
