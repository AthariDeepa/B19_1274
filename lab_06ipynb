    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from sklearn.preprocessing import MinMaxScaler, StandardScaler\n",
        "# Step 1: Create a sample dataset\n",
        "data = {\n",
        "    'CustomerID': [1001, 1002, 1003, 1004, 1005],\n",
        "    'Income': [25000, 48000, 32000, 76000, 54000]  # income in USD\n",
        "}\n",
        "df = pd.DataFrame(data)\n",
        "print(\"Original Dataset:\\n\")\n",
        "print(df)\n",
        "# Step 2: Apply Min-Max Normalization\n",
        "min_max_scaler = MinMaxScaler()\n",
        "df['Income_MinMax'] = min_max_scaler.fit_transform(df[['Income']])\n",
        "# Step 3: Apply Z-score Standardization\n",
        "z_score_scaler = StandardScaler()\n",
        "df['Income_Zscore'] = z_score_scaler.fit_transform(df[['Income']])\n",
        "# Step 4: Output Result\n",
        "print(\"\\nDataset with Scaled Income:\\n\")\n",
        "print(df)"
      ],
